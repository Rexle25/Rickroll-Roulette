{
  "manifest_version": 3,
  "name": "Rickroll Roulette",
  "version": "1.1",
  "description": "With an adjustable probability you will get rickrolled!",
  "permissions": ["storage"],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "options_page": "options.html"
}
